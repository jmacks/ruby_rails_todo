<div class="list-show">
<p><%= @user.username %></p>
  <h1><%= @list.title %></h1>
  <ul>
    <% @list.todos.each do |todo| %>
    <li>
      <%= todo.content %>
    </li>
    <% end %>
  </ul>

<h3>Add a ToDo</h3>

<%= form_for @todo, url: user_list_todos_path(@user.id, @list) do |f| %>
<%= f.text_field :content %>
<%= f.submit "add todo" %>
<% end %>




</div>
<%= link_to 'HOME', root_path %>
